PS1="${STY:+\[\033[4;36m\]screen:${STY%%.*}>\[\033[24;00m\] }[\[\033[0;32m\]\u\[\033[0;33m\]@\h\[\033[00m\] \[\033[01;00m\]\w\[\033[00m\]]\$ "

export LC_TIME="en_US.UTF8"

# my own script/bin path
export PATH="~/bin:$PATH"

alias ll='ls -lh'
alias d1='du -h --max-depth=1'
alias d1k='du -k --max-depth=1'
alias rsync='rsync --progress'

alias fac="grep -c '>' "
alias fah="grep '>' "
alias wl="wc -l "


